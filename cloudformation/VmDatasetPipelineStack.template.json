    "Code": {
     "ZipFile": "import boto3\nimport json\nimport os\n\nGENERATORS = [\n    'O-41_nonogram_data-generator',\n    'O-42_object_permanence_data-generator',\n    'O-43_object_subtraction_data-generator',\n    'O-55_rotation_data-generator'\n]\n\ndef handler(event, context):\n    sqs = boto3.client('sqs')\n    queue_url = os.environ['QUEUE_URL']\n    \n    generators = event.get('generators', GENERATORS)\n    samples = event.get('samples', 10000)\n    batch_size = event.get('batch_size', 25)\n    output_format = event.get('output_format', 'tar')\n    \n    count = 0\n    for gen in generators:\n        for start in range(0, samples, batch_size):\n            sqs.send_message(\n                QueueUrl=queue_url,\n                MessageBody=json.dumps({\n                    'type': gen,\n                    'start_index': start,\n                    'num_samples': min(batch_size, samples - start),\n                    'seed': start,\n                    'output_format': output_format\n                })\n            )\n            count += 1\n    \n    return {'messages_sent': count, 'generators': len(generators), 'samples_per_gen': samples}\n"
    }
   },
   "DependsOn": [
    "SubmitTasksRole"
   ]
  }
